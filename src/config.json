            "leadTimeEndStatuses": ["Done"],
            "timeformat": "YYYY-MM-DDTHH:mm:zz.SSS+-HHmm",
            "dataUrl": {
                "localUrl": "http://localhost:5000/api",
                "dataSourceUri": "https://jira.schwab.com",
                "headers": {
                    "Authorization": "Basic ***REMOVED***"
                },
                "sprints": {
                    "includedIssueTypes": ["Story","Bug"],
                    "config": {
                        "boundaries": {
                            "boardId": "5519",
                            "startAt": "0",
                            "maxResults": "50",
                            "method": "GET",
                            "url": "/rest/agile/1.0/board/{boardId}/sprint",
                            "jql": "",
                            "expand": "",
                            "fields": "",
                            "timeout": ""
                        },
                        "issues": {
                            "startAt": "0",
                            "maxResults": "25",
                            "method": "GET",
                            "getSubtasks": true,
                            "url": "/rest/agile/1.0/sprint/{sprintId}/issue",
                            "jql": "",
                            "expand": "changelog",
                            "fields": "summary,issuetype,resolutiondate,created,project,labels",
                            "timeout": 180000
                        }
                    }
                },
                "issues": {
                    "config": {
                        "startAt": "0",
                        "maxResults": "25",
                        "method": "GET",
                        "getSubtasks": true,
                        "url": "/rest/api/2/search",
                        "jql": "'Team (Schwab)' = 'Ice Dragons' and resolved >='2019-04-01' and issuetype = Story",
                        "expand": "changelog",
                        "fields": "summary,issuetype,resolutiondate,created,project,labels",
                        "timeout": 180000
                    }
                },
                "issue": {
                    "config": {
                        "url": "/rest/api/2/issue/{key}",
                        "expand": "changelog",
                        "fields": "summary,issuetype,resolutiondate,created,project,labels",
                        "subtasks": {
                            "url": "/subtask"
                        }
                    }
                }
            }
        },
